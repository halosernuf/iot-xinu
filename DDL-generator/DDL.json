	{
		"Description":{
			"Name":"tmp36",
			"Device_Type":"Sensor",
			"Verbose_Description":"TMP36 Analog Temperature Sensor"
		},
		"Interface":{
			"Signal":[
				{
					"id":"s1",
					"Operation":"Input",
					"Type":"Analog",
					"Measurement":"ADC"
				}
			],
			"Reading":[
				{
					"Type":"Sensor",
					"From":"s1",
					"Measurement":"Temperature",
					"Unit":"Centigrade",
					"Data_Type":"uint32",
					"Return_Length":"1",
					"Computation":{
						"Type":"Formula",
						"Expression": [
							"s1[0] = (((s1[0]*1.8)/4095)-0.5)*100"
						]
					}
				}
			]
		}
	},
